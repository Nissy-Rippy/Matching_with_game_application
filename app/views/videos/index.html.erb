  <div class="container">
    <div class="row">
      <div class="col-sm-12 col-md-12">
        <div class="search-container text-center">
      　　  <%= form_with url: search_videos_path, local: true, method: :get do |f| %>
      　　   <%= f.text_field :key_word, value: @key_word, placeholder: "検索", id: "searching-form" %>
      　　   <%= f.submit "検索" %>
      　　<% end %>
      　　   <div class="result">
      　　     <ul></ul>
      　　   </div>
    　　</div>
    　　<h2 class="text-center mb-5">動画一覧ページ</h2>
    　　<h3 class="text-center">あなたのお気に入りは・・・</h3>
    　　<%= render "videos/index", videos: @videos %>
      </div>
    </div>
  </div>